{
  "extends": "@eugenios/typescript-config/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@eugenios/ui/*": ["../../packages/ui/*"],
      "@eugenios/types/*": ["../../packages/types/*"],
      "@eugenios/react-query/*": ["../../packages/react-query/*"],
      "@eugenios/services/*": ["../../packages/services/*"],
      "@eugenios/icons/*": ["../../packages/icons/*"],

    },
    "types": ["node", "next/image-types/global"]
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx", ".next/types/**/*.ts", "next.config.mjs"],
  "exclude": ["node_modules", ".next", "dist"]
}
